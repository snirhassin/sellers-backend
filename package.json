{
  "name": "sellers-backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "cd frontend && npm install && npm run build && cd .. && prisma generate",
    "railway:deploy": "prisma db push && npm run db:seed",
